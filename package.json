{
  "name": "dubbo",
  "version": "1.0.4",
  "description": "node.js dubbo/dubbox client with zookeeper support via dubbo default hessian protocol",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "npm run build && node --harmony ./node_modules/.bin/ava --verbose --timeout=10s",
    "prepublish": "npm run build"
  },
  "files": [
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/node-dubbo.git"
  },
  "keywords": [
    "dubbo",
    "dubbox",
    "zookeeper",
    "client",
    "dubbo-client",
    "rpc",
    "hessian",
    "pool"
  ],
  "engines": {
    "node": ">=4"
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/node-dubbo/issues"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    },
    "files": [
      "test/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "hessian.js": "^2.8.1",
    "ip": "^1.1.5",
    "node-zookeeper-client": "^0.2.2",
    "once": "^1.4.0",
    "semver": "^5.5.0",
    "socket-pool": "^1.2.2"
  }
}
